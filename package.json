{
  "name": "swc-server",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "concurrently \"pnpm watch-compile\" \"pnpm watch-dev\"",
    "watch-compile": "swc src -w -d dist --config-file ./.swcrc",
    "watch-dev": "nodemon --watch \"dist/**/*\" -e js ./dist/src/server.js",
    "build": "swc src -d dist --config-file ./.swcrc",
    "start": "node dist/src/server.js",
    "clean": "rm -rf dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.3"
  },
  "devDependencies": {
    "@swc/cli": "^0.3.10",
    "@swc/core": "^1.4.2",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "chokidar": "^3.6.0",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.0"
  }
}
