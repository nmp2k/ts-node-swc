{
  "exclude": ["\/types\/",".*\\.js$", ".*\\.map$", ".*\\.d\\.ts$"],
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true,
      "dynamicImport": true
    },
    "target": "es2020",
    "paths": {
      "~/*": ["src/*"],
    },
    "baseUrl": "."
  },
  "module": {
    "type": "commonjs",
    "strictMode": true,
    "resolveFully": true,
    "noInterop": false
  }
}