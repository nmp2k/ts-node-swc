{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true,
      "dynamicImport": true
    },
    "target": "es2020",
    "paths": {
      "~/*": ["src/*"]
    },
    "baseUrl": "."
  },
  "module": {
    "type": "commonjs",
    "noInterop": false,
    "strictMode": true,
    "resolveFully": true
  }
}